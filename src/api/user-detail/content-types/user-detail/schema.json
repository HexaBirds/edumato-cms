{
  "kind": "collectionType",
  "collectionName": "user_details",
  "info": {
    "singularName": "user-detail",
    "pluralName": "user-details",
    "displayName": "user-detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "organisationName": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "long": {
      "type": "float"
    },
    "lati": {
      "type": "float"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "isApproved": {
      "type": "boolean",
      "default": false
    },
    "showDetail": {
      "type": "boolean",
      "default": false
    },
    "subjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subject.subject",
      "mappedBy": "user_details"
    },
    "favorites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::favorite.favorite",
      "mappedBy": "user_detail"
    },
    "like": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::like-fav.like-fav",
      "inversedBy": "user_details"
    },
    "user_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "user_detail"
    },
    "comment_reviews": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::comment-review.comment-review",
      "mappedBy": "user_details"
    }
  }
}
