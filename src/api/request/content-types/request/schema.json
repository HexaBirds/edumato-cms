{
  "kind": "collectionType",
  "collectionName": "requests",
  "info": {
    "singularName": "request",
    "pluralName": "requests",
    "displayName": "request"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "requests"
    },
    "user_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-detail.user-detail",
      "mappedBy": "request"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "resolved",
        "declined"
      ],
      "default": "pending"
    },
    "phone": {
      "type": "string"
    }
  }
}
